{
    "name": "dtrace",
    "displayName": "DTrace",
    "description": "DTrace language extension for VSCode",
    "version": "0.2.0",
    "publisher": "cedrou",
    "repository": {
        "type": "git",
        "url": "https://github.com/cedrou/vscode-dtrace.git"
    },
    "bugs": {
        "url": "https://github.com/cedrou/vscode-dtrace"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "dscript",
                "aliases": [
                    "D script",
                    "dscript"
                ],
                "extensions": [
                    ".d",
                    ".dtrace"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "dscript",
                "scopeName": "source.dtrace",
                "path": "./syntaxes/dscript.tmLanguage.json"
            }
        ]
    }
}